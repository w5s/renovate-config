{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Ignore paths with special cases (ionic, RN, etc)",
  "ignorePaths": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/vendor/**",
    "**/examples/**",
    "**/__tests__/**",
    "**/test/**",
    "**/tests/**",
    "**/__fixtures__/**",
    "**/.modules/**",
    "android/**",
    "ios/**"
  ]
}
