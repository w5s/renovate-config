{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Automerge for NPM dependencies",
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchDepTypes": ["dependencies"],
      "matchUpdateTypes": ["minor", "patch"],
      "matchPackageNames": ["aws-sdk"],
      "automerge": true
    }
  ]
}
