{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Automerge Jest",
  "packageRules": [
    {
      "matchPackageNames": ["@types/jest", "babel-jest", "ts-jest", "expect"],
      "matchPackagePrefixes": ["jest", "@jest"],
      "labels": ["AUTOMERGE"],
      "automerge": true
    }
  ]
}
